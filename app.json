{
  "expo": {
    "name": "Papote",
    "slug": "papote",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.steflavoie.papote",
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.steflavoie65.papote",
      "newArchEnabled": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "papote",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "L'application souhaite accéder à vos photos pour les partager"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Autoriser Papote à accéder à vos photos.",
          "savePhotosPermission": "Autoriser Papote à sauvegarder les photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Papote to use your location.",
          "locationWhenInUsePermission": "Allow Papote to use your location while the app is in use."
        }
      ]
    ]
  }
}
